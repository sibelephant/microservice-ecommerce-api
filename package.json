{ "name": "microservice-ecommerce-api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node gateway/server.js",
    "start:users": "node services/user-service/server.js",
    "start:products": "node services/product-service/server.js",
    "start:orders": "node services/order-service/server.js",
    "dev": "concurrently \"npm run start:users\" \"npm run start:products\" \"npm run start:orders\" \"npm run start\""
  },
  "dependencies": {
    "express": "^5.1.0",
      "cors": "^2.8.5",
    "helmet": "^6.0.1",
    "http-proxy-middleware": "^2.0.6",
    "axios": "^1.3.4",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "uuid": "^9.0.0",
    "concurrently": "^7.6.0"
  }
}
